    "import pyqrcode\n",
    "import png  \n",
    "# Define the data (your GitHub profile URL)\n",
    "data = \"https://github.com/ankush850\"\n",
    "\n",
    "# Create the QR code\n",
    "qr = pyqrcode.create(data)\n",
    "\n",
    "# Save the QR code in PNG format\n",
    "qr.png(\"githubID.png\", scale=5)\n",
    "\n",
    "print(\"QR code generated and saved as githubID.png\")"
   ]
  }
